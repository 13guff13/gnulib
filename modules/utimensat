Description:
Set file access and modification times of a file relative to a directory fd.

Files:
lib/utimensat.c
lib/at-func.c
m4/utimensat.m4

Depends-on:
at-internal
dosname
errno
extensions
fchdir
fcntl-h
openat-die
openat-h
save-cwd
sys_stat
fstatat         [test $HAVE_UTIMENSAT = 0 || test $REPLACE_UTIMENSAT = 1]
utimens         [test $HAVE_UTIMENSAT = 0 || test $REPLACE_UTIMENSAT = 1]

configure.ac:
gl_FUNC_UTIMENSAT
if test $HAVE_UTIMENSAT = 0 || test $REPLACE_UTIMENSAT = 1; then
  AC_LIBOBJ([utimensat])
fi
gl_SYS_STAT_MODULE_INDICATOR([utimensat])

Makefile.am:

Include:
<sys/stat.h>

Link:
$(LIB_CLOCK_GETTIME)

