execute
xsetenv
sh-quote
xalloc
xallocsa
error
gettext-h

configure.ac:
# You need to invoke gt_CSHARPEXEC yourself, possibly with arguments.
AC_CONFIG_FILES([csharpexec.sh:build-aux/csharpexec.sh.in])

Makefile.am:
DEFS += -DEXEEXT=\"$(EXEEXT)\"
lib_SOURCES += csharpexec.h csharpexec.c
EXTRA_DIST += classpath.h classpath.c

